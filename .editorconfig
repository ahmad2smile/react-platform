# EditorConfig is awesome: http://EditorConfig.org
root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[gulpfile.js]
indent_style = tabs
indent_size = 4

[*.scss]
indent_style = tabs
indent_size = 4

[*.json]
indent_style = tabs
indent_size = 4
